<?xml version="1.0" encoding="utf-8"?>
<layer-list
        xmlns:android="http://schemas.android.com/apk/res/android">
    <item
            android:id="@android:id/background">
        <shape>
            <corners
                    android:radius="25dip" />
            <solid
                    android:color="#916767"/>
        </shape>
    </item>
    <item
            android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners
                        android:radius="25dip" />
                <gradient
                        android:startColor="#9acfeb"
                        android:endColor="#9acfeb"
                        android:angle="90" />
            </shape>
        </clip>
    </item>
    <item
            android:id="@android:id/progress">
        <clip>
            <shape>
                <corners
                        android:radius="25dip" />
                <gradient
                        android:startColor="#9acfeb"
                        android:endColor="#9acfeb"
                        android:angle="90" />
            </shape>
        </clip>
    </item>
</layer-list>